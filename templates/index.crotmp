<!DOCTYPE html>
<html>
    <head>
        <script src="https://unpkg.com/frappe-charts@1.2.4/dist/frappe-charts.min.iife.js"></script>
        <script type="module" src="/js/frappe_chart.js"></script>
    </head>
    <body>
        <h1>Logs</h1>

        <div id="chart"></div>

        <div>
            <select name="projects" id="projects">
                <option value="1" selected>intranet</option>
                <@projects>
                    <?{.name ne 'intranet' }>
                        <option value="<.id>"><.name></option>
                    </?>
                </@>
            </select>
        </div>
        <div>
            <select name="test_file" id="test_files">
                <option value="">Test file</option>
                <@project-data.test_files>
                    <option value="<.id>"><.name></option>
                </@>
            </select>
        </div>

        <div>
            <table id="test_table">
                <thead>
                    <tr>
                        <th>Test File</th>
                        <th>Test number</th>
                        <th>Date</th>
                    </rt>
                </thead>
                <tbody id="test_table_body">
                        <@project-data.numbers>
                            <tr>
                                <td><.name></td>
                                    <@.tests>
                                        <td><.[0]></td>
                                        <td><.[1]></td>
                                    </@>
                            </tr>
                        </@>
                </tbody>
            </table>
        </div>
    </body>
</html>
